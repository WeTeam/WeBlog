@model Sitecore.Modules.WeBlog.Model.FacebookLike

<div class="wb-facebook wb-panel">
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'div', 'fb-root'));
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    <div class="fb-like" data-layout="@Model.LayoutStyle" data-href="@Model.UrlToLike" data-send="@Model.SendButton" data-width="@Model.Width" data-colorscheme="@Model.ColorScheme" data-show-faces="@Model.ShowFaces"></div>
</div>